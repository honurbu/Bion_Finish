@using Blogss.Models.Context
@model Blogss.Models.Organisation



@{
    ViewData["Title"] = "AddOrganisation";
    Layout = "~/Areas/Adminss/Views/Shared/View.cshtml";
}

<h1>AddOrganisation</h1>


@using (Html.BeginForm("AddOrganisation", "AdminOrganisiation", FormMethod.Post))
{
 

    @Html.Label("Organizasyon Adı ")
    @Html.TextBoxFor(x => x.OrganisationName, new { @class = "form-control" })
    <br />



    @Html.Label("Organizasyon Tanıtımı Başlığı")
    @Html.TextBoxFor(x => x.Definition, new { @class = "form-control" })
    <br />




    <br />



    <br />

    <br />
    <br />

    <button class="btn btn-success" id="btn1">Ekle</button>

}